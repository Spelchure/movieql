    "postman",
    "express",
    "jest"
  ],
  "author": "Alperen BAYRAMOÄžLU",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Spelchure/movieql/issues"
  },
  "homepage": "https://github.com/Spelchure/movieql#readme",
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.5",
    "@types/chai-subset": "^1.3.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
